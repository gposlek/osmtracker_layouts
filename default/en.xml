
<layouts>
  <layout name="root">
      <row>
        <button type="textnote" icon="text_32x32" />
        <button type="voicerec" icon="voice_32x32" />
        <button type="picture" icon="camera_32x32" />
      </row>
      <row>
        <button type="page" label="max_speed" icon="default_icons/HR_road_sign_B30_(30).png" targetlayout="max_speed_layout"/>
        <button type="page" label="warning" icon="default_icons/HR_road_sign_A01.png" targetlayout="warning_layout"/>
      </row>
  </layout>
  <layout name="max_speed_layout">
      <row>
        <button type="tag" label="max" icon="default_icons/HR_road_sign_B30_(30).png"/>
        <button type="tag" label="max" icon="default_icons/HR_road_sign_B30_(40).png"/>
      </row>
  </layout>
  <layout name="warning_layout">
      <row>
        <button type="tag" label="_" icon="default_icons/HR_road_sign_A10.png"/>
        <button type="tag" label="_" icon="default_icons/HR_road_sign_A13.png"/>
        <button type="tag" label="_" icon="default_icons/HR_road_sign_A33.png"/>
      </row>
  </layout>
</layouts>
